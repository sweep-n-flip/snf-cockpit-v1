    "@payloadcms/plugin-seo": "3.0.0-beta.73",
    "@payloadcms/richtext-lexical": "3.0.0-beta.73",
    "@payloadcms/storage-vercel-blob": "3.0.0-beta.73",
    "@tanstack/react-query": "^5.50.1",
    "classnames": "^2.5.1",
    "connectkit": "^1.8.2",
    "cross-env": "^7.0.3",
    "ethers": "^6.13.1",
    "graphql": "^16.8.1",
    "husky": "^9.0.11",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "next": "15.0.0-canary.58",
    "payload": "3.0.0-beta.73",
    "rc-slider": "^10.6.2",
    "react": "^19.0.0-rc-f994737d14-20240522",
    "react-dom": "^19.0.0-rc-f994737d14-20240522",
    "react-hook-form": "^7.52.1",
